a.anchor#kontakt(href="javascript:void(0)" title="Kontakt")
section#kontakt_section
  .map
    iframe.map__frame(src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCA1uDuyDOVj-PZ1PxRZ3HgSek0rpYOeBQ&q=Oporowo+18,+64-560+Ostroróg")
  .contact
    p.contact__heading="Kontakt"
    .contact__container
      p.contact__name="Moskito"
      dl.contact__list
        dt="Imię i nazwisko"
        dd="Sebastian Szymczak"
        dt="Adres"
        dd="Oporowo 18/3"
        dd="64-560 Ostroróg"
        dt="Adres e-mail"
        dd
          a(href="mailto:"+emai1)=email1
        dt="Telefon"
        dd
          a(href="tel:" + phone.replace(' ',''))="+48 "+phone